			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml-schemas</artifactId>
			<version>3.17</version>
		</dependency>


	</dependencies>
	<build>
		<filters>
			<filter>src/main/resources/application.yml</filter>
			<filter>src/main/resources/config/application-${pom.env}.yml</filter>
		</filters>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<includes>
					<include>**/*</include>
				</includes>
				<filtering>true</filtering>
			</resource>
		</resources>
		<testResources>
			<testResource>
				<directory>src/test/resources</directory>
				<filtering>true</filtering>
			</testResource>
		</testResources>

		<plugins>
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<configuration>
					<!-- 没有该配置，devtools 不生效 -->
					<fork>true</fork>
					<addResources>true</addResources>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-resources-plugin</artifactId>
				<configuration>
					<encoding>UTF-8</encoding>
					<useDefaultDelimiters>true</useDefaultDelimiters>
				</configuration>
			</plugin>
		</plugins>

	</build>

	<profiles>
		<profile>
			<id>dev</id>
			<properties>
				<pom.env>dev</pom.env>
			</properties>
			<activation>
				<activeByDefault>true</activeByDefault>
				<property>
					<name>APP_ENV</name>
					<value>dev11</value>
				</property>
			</activation>
		</profile>
		<profile>
			<id>test</id>
			<properties>
				<pom.env>test</pom.env>
			</properties>
			<activation>
				<property>
					<name>APP_ENV</name>
					<value>test</value>
				</property>
			</activation>
		</profile>
		<profile>
			<id>idc_test</id>
			<properties>
				<pom.env>idc_test</pom.env>
			</properties>
			<activation>
				<property>
					<name>APP_ENV</name>
					<value>idc_test</value>
				</property>
			</activation>
		</profile>
		<profile>
			<id>idc_product</id>
			<properties>
				<pom.env>idc_product</pom.env>
			</properties>
			<activation>
				<property>
					<name>APP_ENV</name>
					<value>idc_product</value>
				</property>
			</activation>
		</profile>
	</profiles>

	<repositories>
		<repository>
			<id>Hudong-Local</id>
			<url>http://mvn2.hudong.net:8080/artifactory/Hudong-Local</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>Third-Party</id>
			<url>http://mvn2.hudong.net:8080/artifactory/Third-Party</url>
		</repository>
		<repository>
			<id>remote-repos</id>
			<url>http://mvn2.hudong.net:8080/artifactory/remote-repos</url>
		</repository>
	</repositories>

</project>
